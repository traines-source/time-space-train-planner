          },
          {
            "name": "transferTime",
            "in": "query",
            "description": "Minimum time in minutes for a single transfer.",
                          "type": "integer",
              "default": 0

          },
          {
            "name": "accessibility",
            "in": "query",
            "description": "`partial` or `complete`. – Default: *not accessible*",
